generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Log {
  id           String   @id @default(uuid()) @map("_id") @db.ObjectId
  log          String
  deploymentId String
  timestamp    DateTime
  type         LogType
}

enum LogType {
  Info
  Error
}
