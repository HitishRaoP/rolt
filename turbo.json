{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "!.next/cache/**", "dist"]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"check-types": {
			"dependsOn": ["^check-types"],
			"cache": true
		},
		"test": {
			"dependsOn": ["^test"]
		},
		"bootstrap": {
			"dependsOn": ["^bootstrap"]
		}
	}
}
